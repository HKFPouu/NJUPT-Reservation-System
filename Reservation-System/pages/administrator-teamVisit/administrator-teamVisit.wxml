<view class="administrator-teamVisit">
  <view class="administrator-teamVisit-listBox" wx:for-items="{{applyList}}" wx:for-index="key"  wx:for-item="item">
    <view class="administrator-teamVisit-list flex" catchtap = "specificInformation" id="item{{key}}" >
      <text>{{item.date}}</text>
      <text>{{item.time}}</text>
      <text>{{item.contactMan}}</text>
      <text>{{item.status}}</text>
    </view>
  </view>
</view>