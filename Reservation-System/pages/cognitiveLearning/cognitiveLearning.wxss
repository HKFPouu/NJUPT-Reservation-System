.cognitiveLearning{
  background: #f5f5f5;
}

.cognitiveLearningForm,
.cognitiveLearningGuest,
.contact {
  width: 86%;
  padding-left: 5%;
  padding-right: 5%;
  margin: 0 auto;
  background: #ffffff;
}

.cognitiveLearningGuest,.contact {
  margin-top: 3vh;
}

.cognitiveLearningGuestText {
  text-align: center;
  font-size: 35rpx;
  font-weight: bold;
  padding: 2vh 0;
}

.cognitiveLearningSubmit {
  border: 1px solid #ddd;
  background: #fffffF;
  font-size: 30rpx;
  font-weight: 500;
  margin: 3vh 0;
  border-radius: 20rpx;
}